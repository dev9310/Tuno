{
    "builds": [{
        "src": "musicplayer/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/(.*)",
            "dest": "musicplayer/wsgi.py"
        }
    ]
}