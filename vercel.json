{
    "version": 2,
    "builds": [{
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "staticfiles_build/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [{
            "src": "/static/(.*)",
            "dest": "staticfiles_build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}