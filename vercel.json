{
    "version": 2,
    "builds": [{
            "src": "manage.py",
            "use": "@vercel/python"
        },
        {
            "src": "staticfiles_build/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [{
            "src": "/static/(.*)",
            "dest": "staticfiles_build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "manage.py"
        }
    ],
    "env": {
        "SECRET_KEY": "your-secret-key",
        "DEBUG": "true",
        "DB_NAME": "postgres",
        "DB_USER": "your-db-user",
        "DB_PASSWORD": "your-db-password",
        "DB_HOST": "your-db-host",
        "DB_PORT": "6543"
    }
}